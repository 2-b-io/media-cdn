{
  "name": "media-on-demand-cdn",
  "version": "1.0.0",
  "description": "Easy-integrated online service to create media real time from original media",
  "main": "index.js",
  "repository": "git@github.com:MediaNetwork/media-on-demand-cdn.git",
  "author": "LongLH <d@dapps.me>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "2.191.0",
    "bluebird": "3.5.1",
    "boolean": "0.1.3",
    "download": "6.2.5",
    "express": "4.16.2",
    "mime": "2.2.0",
    "mkdirp": "0.5.1",
    "mongoose": "5.0.4",
    "morgan": "1.9.0",
    "nocache": "2.0.0",
    "object.pick": "1.3.0",
    "one-doing-the-rest-waiting": "1.0.9",
    "parse-domain": "2.0.0",
    "recursive-config": "1.0.1",
    "sharp": "0.19.0",
    "short-hash": "1.0.0",
    "url-join": "4.0.0",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "concurrently": "3.5.0",
    "nodemon": "1.12.1"
  },
  "scripts": {
    "start": "concurrently \"yarn dev:server\" \"yarn dev:worker\"",
    "build": "cd app && babel . -d ../build",
    "build:clean": "rm -rf build",
    "dev:server": "cd app && nodemon server/ --exec babel-node --CONFIG=../internals/config.dev.js",
    "dev:worker": "cd app && nodemon workers/media/ --exec babel-node --CONFIG=../internals/config.dev.js",
    "staging:start": "concurrently \"yarn staging:server\" \"yarn staging:worker\"",
    "staging:server": "cd build && node server/ --CONFIG=../internals/config.dev.js",
    "staging:worker": "cd build && node workers/media/ --CONFIG=../internals/config.dev.js"
  }
}
