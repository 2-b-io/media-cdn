{
  "name": "media-on-demand",
  "version": "1.0.0",
  "description": "Easy-integrated online service to create media real time from original media",
  "main": "index.js",
  "repository": "git@github.com:MediaSocialNetwork/media-on-demand.git",
  "author": "LongLH <d@dapps.me>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "2.125.0",
    "bluebird": "3.5.0",
    "download": "6.2.5",
    "express": "4.16.1",
    "gm": "1.23.0",
    "ioredis": "3.1.4",
    "kue": "0.11.6",
    "mime": "2.0.3",
    "nocache": "2.0.0",
    "short-hash": "1.0.0",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "concurrently": "3.5.0",
    "nodemon": "1.12.1"
  },
  "scripts": {
    "start": "concurrently \"yarn run worker\" \"yarn run server\"",
    "worker": "nodemon ./src/workers/media.js --ignore node_modules --ext js",
    "server": "nodemon ./src/server/index.js --ignore node_modules --ext js"
  }
}
