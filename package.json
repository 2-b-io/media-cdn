{
  "name": "@media-network/cdn",
  "version": "1.0.0",
  "description": "Easy-integrated online service to create media real time from original media",
  "main": "index.js",
  "repository": "git@github.com:media-network/media-cdn.git",
  "author": "LongLH <d@dapps.me>",
  "license": "MIT",
  "dependencies": {
    "amqplib": "0.5.2",
    "array-flatten": "2.1.1",
    "async": "2.6.0",
    "aws-sdk": "2.191.0",
    "bluebird": "3.5.1",
    "boolean": "0.1.3",
    "clean-css": "4.1.9",
    "debug": "3.1.0",
    "deferred": "0.7.9",
    "delay": "2.0.0",
    "download": "6.2.5",
    "express": "4.16.2",
    "fs-extra": "5.0.0",
    "got": "8.3.0",
    "imagemin": "5.3.1",
    "imagemin-pngquant": "5.1.0",
    "mime": "2.2.0",
    "mime-match": "1.0.2",
    "minify-stream": "1.1.0",
    "mkdirp": "0.5.1",
    "mongoose": "5.0.4",
    "morgan": "1.9.0",
    "ms": "2.1.1",
    "multer": "1.3.0",
    "mv": "2.1.1",
    "newrelic": "3.2.0",
    "one-doing-the-rest-waiting": "1.0.9",
    "recursive-config": "1.0.1",
    "serialize-error": "2.1.0",
    "sharp": "0.19.0",
    "short-hash": "1.0.0",
    "shorthash": "0.0.2",
    "superagent": "3.8.2",
    "uglify-js": "3.3.12",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "concurrently": "3.5.0",
    "nodemon": "1.12.1"
  },
  "scripts": {
    "up": "docker-compose up -d && yarn remote",
    "remote": "docker exec -it media.cdn bash",
    "start": "NEW_RELIC_ENABLED=false concurrently --raw \"yarn dev:server\" \"yarn dev:worker\"",
    "build": "cd src && babel . -d ../build",
    "build:clean": "rm -rf build",
    "dev:server": "cd src && nodemon server/ --exec babel-node --CONFIG=../internals/config.dev.js",
    "dev:worker": "cd src && nodemon workers/ --exec babel-node --CONFIG=../internals/config.dev.js",
    "staging:start": "concurrently \"yarn staging:server\" \"yarn staging:worker\"",
    "staging:server": "cd build && node server/ --CONFIG=../internals/config.dev.js",
    "staging:worker": "cd build && node workers/media/ --CONFIG=../internals/config.dev.js"
  }
}
