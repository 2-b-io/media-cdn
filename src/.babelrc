{
  "presets": [
    [ "@babel/preset-env", {
      "targets": {
        "node": "8.10"
      }
    } ]
  ],
  "plugins": [
    [ "module-resolver", {
      "root": [ "./src" ],
    } ],
    [ "@babel/plugin-transform-runtime", {
      "regenerator": true
    } ],
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
