{
  "name": "message-bus",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn p1\" \"yarn p2\" \"yarn c1\" \"yarn c2\" \"yarn h1\" \"yarn h2\"",
    "p1": "nodemon test/p1.js --exec babel-node --watch src --watch test/p1.js",
    "p2": "nodemon test/p2.js --exec babel-node --watch src --watch test/p2.js",
    "c1": "nodemon test/c1.js --exec babel-node --watch src --watch test/c1.js",
    "c2": "nodemon test/c2.js --exec babel-node --watch src --watch test/c2.js",
    "h1": "nodemon test/h1.js --exec babel-node --watch src --watch test/h1.js",
    "h2": "nodemon test/h2.js --exec babel-node --watch src --watch test/h2.js"
  }
}
